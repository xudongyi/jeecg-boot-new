    <select id="queryAirqHourMonitor" resultMap="AirqHourMonitorVO">
<!--        select a.mn,a.site_name,b.`level` as airLevel,b.level,a.area,b.aqi,-->
<!--               max(b.CREATE_TIME) create_time,b.first_code,c.meaning,-->
<!--				b.a21026_avg,b.a21004_avg,b.a3400201_avg,b.a3400224_avg,-->
<!--				b.a21005_avg,b.a0502401_avg,b.a0502408_avg,b.a3400401_avg,-->
<!--				b.a3400424_avg,b.a01002_avg,b.a01007_avg,b.a01006_avg,b.A01008_AVG-->

<!--		from airq_hour b  left join site_monitor_point a on  b.mn = a.mn-->
<!--        left join sys_pollution_code c on b.FIRST_CODE = c.code-->
<!--        where 1=1-->
<!--        <if test="area != null">-->
<!--            and a.area = (#{area})-->
<!--        </if>-->
<!--        <if test="mn != null">-->
<!--            and a.mn = (#{mn})-->
<!--        </if>-->
<!--        <if test="dateBegin != null">-->
<!--            and b.create_time >= (#{dateBegin})-->
<!--        </if>-->
<!--        <if test="dateEnd != null">-->
<!--            and b.create_time &lt;= (#{dateEnd})-->
<!--        </if>-->
<!--      	group by a.site_name-->
<!--        order by b.aqi asc-->

        select t1.* from (select a.mn,a.site_name,b.`level` as airLevel,b.level,a.area,b.aqi,
        max(b.data_time) data_time,b.first_code,c.meaning,
        b.a21026_avg,b.a21004_avg,b.a3400201_avg,b.a3400224_avg,
        b.a21005_avg,b.a0502401_avg,b.a0502408_avg,b.a3400401_avg,
        b.a3400424_avg,b.a01001_avg,b.a01002_avg,b.a01007_avg,b.a01006_avg,b.A01008_AVG

        from airq_hour b  left join site_monitor_point a on  b.mn = a.mn
        left join sys_pollution_code c on b.FIRST_CODE = c.code WHERE b.state in (0,1)
        group by a.site_name
        order by b.aqi asc,b.data_time desc) t1
        where 1=1
        <if test="area != null">
            and t1.area = (#{area})
        </if>
        <if test="mn != null">
            and t1.mn = (#{mn})
        </if>
        <if test="dateBegin != null">
            and t1.data_time >= (#{dateBegin})
        </if>
        <if test="dateEnd != null">
            and t1.data_time &lt;= (#{dateEnd})
        </if>

    </select>

